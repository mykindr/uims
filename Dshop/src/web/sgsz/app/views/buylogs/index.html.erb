<h1><%= t 'views.buylogs.index.listing_buylogs' %></h1>

<table>
  <tr>
    <th>Uid</th>
    <th>Uname</th>
    <th>Utel</th>
    <th>Cpbh</th>
    <th>Cpmc</th>
    <th>Ys</th>
    <th>Sl</th>
    <th>Js</th>
    <th>Dj</th>
    <th>Yfbz</th>
    <th>Xj</th>
    <th>Tj</th>
    <th>Bz</th>
    <th>Cdate</th>
    <th>Remark</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @buylogs.each do |buylog| %>
  <tr>
    <td><%= buylog.uid %></td>
    <td><%= buylog.uname %></td>
    <td><%= buylog.utel %></td>
    <td><%= buylog.cpbh %></td>
    <td><%= buylog.cpmc %></td>
    <td><%= buylog.ys %></td>
    <td><%= buylog.sl %></td>
    <td><%= buylog.js %></td>
    <td><%= buylog.dj %></td>
    <td><%= buylog.yfbz %></td>
    <td><%= buylog.xj %></td>
    <td><%= buylog.tj %></td>
    <td><%= buylog.bz %></td>
    <td><%= buylog.cdate %></td>
    <td><%= buylog.remark %></td>
    <td><%= link_to t('views.show'), buylog %></td>
    <td><%= link_to t('views.edit'), edit_buylog_path(buylog) %></td>
    <td><%= link_to t('views.destroy'), buylog, method: :delete, data: { confirm: t('views.are_you_sure') } %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @buylogs, :window => 2 %>
<br />

<%= link_to t('views.buylogs.index.new_buylogs'), new_buylog_path %>
