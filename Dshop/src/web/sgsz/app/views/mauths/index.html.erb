<h1><%= t 'views.mauths.index.listing_mauths' %></h1>

<table>
  <tr>
    <th><%= t 'views.mauths.index.uid' %></th>
    <th><%= t 'views.mauths.index.rid' %></th>
    <th><%= t 'views.mauths.index.mid' %></th>
    <th><%= t 'views.mauths.index.cdkey' %></th>
    <th><%= t 'views.mauths.index.result' %></th>
    <th><%= t 'views.mauths.index.cdate' %></th>
    <th><%= t 'views.mauths.index.remark' %></th>
    <th colspan="3"><%= t 'views.operation' %></th>
  </tr>

<% @mauths.each do |mauth| %>
  <tr>
    <td><%= mauth.uid %></td>
    <td><%= mauth.rid %></td>
    <td><%= mauth.mid %></td>
    <td><%= mauth.cdkey %></td>
    <td><%= mauth.result %></td>
    <td><%= mauth.cdate %></td>
    <td><%= mauth.remark %></td>
    <td><%= link_to t('views.show'), mauth %></td>
    <td><%= link_to t('views.edit'), edit_mauth_path(mauth) %></td>
    <td><%= link_to t('views.destroy'), mauth, method: :delete, data: { confirm: t('views.are_you_sure') } %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @mauths, :window => 2 %>
<br />

<%= link_to t('views.mauths.index.new_mauths'), new_mauth_path %>
