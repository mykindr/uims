<h1><%= t 'views.memberprices.index.listing_memberprices' %></h1>

<table>
  <tr>
    <th><%= t 'views.memberprices.index.pid' %></th>
    <th><%= t 'views.memberprices.index.barcode' %></th>
    <th><%= t 'views.memberprices.index.goodsname' %></th>
    <th><%= t 'views.memberprices.index.size' %></th>
    <th><%= t 'views.memberprices.index.color' %></th>
    <th><%= t 'views.memberprices.index.volume' %></th>
    <th><%= t 'views.memberprices.index.unit' %></th>
    <th><%= t 'views.memberprices.index.custid' %></th>
    <th><%= t 'views.memberprices.index.custname' %></th>
    <th><%= t 'views.memberprices.index.custtel' %></th>
    <th><%= t 'views.memberprices.index.startdate' %></th>
    <th><%= t 'views.memberprices.index.enddate' %></th>
    <th><%= t 'views.memberprices.index.hprice' %></th>
    <th><%= t 'views.memberprices.index.cdate' %></th>
    <th><%= t 'views.memberprices.index.remark' %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @memberprices.each do |memberprice| %>
  <tr>
    <td><%= memberprice.pid %></td>
    <td><%= memberprice.barcode %></td>
    <td><%= memberprice.goodsname %></td>
    <td><%= memberprice.size %></td>
    <td><%= memberprice.color %></td>
    <td><%= memberprice.volume %></td>
    <td><%= memberprice.unit %></td>
    <td><%= memberprice.custid %></td>
    <td><%= memberprice.custname %></td>
    <td><%= memberprice.custtel %></td>
    <td><%= memberprice.startdate %></td>
    <td><%= memberprice.enddate %></td>
    <td><%= memberprice.hprice %></td>
    <td><%= memberprice.cdate %></td>
    <td><%= memberprice.remark %></td>
    <td><%= link_to t('views.show'), memberprice %></td>
    <td><%= link_to t('views.edit'), edit_memberprice_path(memberprice) %></td>
    <td><%= link_to t('views.destroy'), memberprice, method: :delete, data: { confirm: t('views.are_you_sure') } %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @memberprices, :window => 2 %>
<br />

<%= link_to t('views.memberprices.index.new_memberprices'), new_memberprice_path %>
