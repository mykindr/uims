<h1><%= t 'views.profitcustomers.index.listing_profitcustomers' %></h1>

<table>
  <tr>
    <th><%= t 'views.profitcustomers.index.custname' %></th>
    <th><%= t 'views.profitcustomers.index.custtel' %></th>
    <th><%= t 'views.profitcustomers.index.profit' %></th>
  </tr>

<% @profitcustomers.each do |profitcustomer| %>
  <tr>
    <td><%= profitcustomer.custname %></td>
    <td><%= profitcustomer.custtel %></td>
    <td><%= profitcustomer.profit %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @profitcustomers, :window => 2 %>
<br />

