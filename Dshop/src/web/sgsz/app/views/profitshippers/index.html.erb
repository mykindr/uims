<h1><%= t 'views.profitshippers.index.listing_profitshippers' %></h1>

<table>
  <tr>
    <th><%= t 'views.profitshippers.index.sname' %></th>
    <th><%= t 'views.profitshippers.index.tel' %></th>
    <th><%= t 'views.profitshippers.index.amount' %></th>
    <th><%= t 'views.profitshippers.index.volume' %></th>
    <th><%= t 'views.profitshippers.index.profit' %></th>
  </tr>

<% @profitshippers.each do |profitshipper| %>
  <tr>
    <td><%= profitshipper.sname %></td>
    <td><%= profitshipper.tel %></td>
    <td><%= profitshipper.amount %></td>
    <td><%= profitshipper.volume %></td>
    <td><%= profitshipper.profit %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @profitshippers, :window => 2 %>
<br />

