<h1><%= t 'views.sellcostproductions.index.listing_sellcostproductions' %></h1>

<table>
  <tr>
    <th><%= t 'views.sellcostproductions.index.pid' %></th>
    <th><%= t 'views.sellcostproductions.index.goodsname' %></th>
    <th><%= t 'views.sellcostproductions.index.color' %></th>
    <th><%= t 'views.sellcostproductions.index.type' %></th>
    <th><%= t 'views.sellcostproductions.index.amount' %></th>
    <th><%= t 'views.sellcostproductions.index.unit' %></th>
  </tr>

<% @sellcostproductions.each do |sellcostproduction| %>
  <tr>
    <td><%= sellcostproduction.pid %></td>
    <td><%= sellcostproduction.goodsname %></td>
    <td><%= sellcostproduction.color %></td>
    <td><%= sellcostproduction.type %></td>
    <td><%= sellcostproduction.amount %></td>
    <td><%= sellcostproduction.unit %></td>
  </tr>
<% end %>
</table>
<br />
<%= paginate @sellcostproductions, :window => 2 %>
<br />

