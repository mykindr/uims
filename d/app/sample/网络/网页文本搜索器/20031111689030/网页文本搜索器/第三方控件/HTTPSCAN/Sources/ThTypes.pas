implementation

Uses SysUtils;

function QuickFindType (id: string; low, high: integer): integer;
var
   mid: integer;
   Delta: integer;
begin
   Result := -1;
   Delta := high - low;
   if Delta >= 0 then begin
      mid := ((Delta) shr 1) + low;
      if id = Types[mid].id then begin
         Result := mid;
      end
      else begin
         if ansicomparestr (id, Types[mid].id) > 0 then begin
            low := mid + 1;
            Result := QuickFindType (id, low, high);
         end
         else if ansicomparestr (id, Types[mid].id) < 0 then begin
            high := mid - 1;
            Result := QuickFindType (id, low, high);
         end;
      end;
   end;
end;


end.
